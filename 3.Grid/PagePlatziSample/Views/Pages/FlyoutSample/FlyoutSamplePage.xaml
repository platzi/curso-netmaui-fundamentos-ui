<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PagePlatziSample.Views.Pages.FlyoutSample.FlyoutSamplePage"
             xmlns:Views="clr-namespace:PagePlatziSample.Views.Pages.FlyoutSample"
             xmlns:local="clr-namespace:PagePlatziSample" 
             Title="FlyoutSamplePage">
    <FlyoutPage.Flyout>
        <Views:FlyoutMenuPage x:Name="flyoutPage" /> 
    </FlyoutPage.Flyout>
    <FlyoutPage.Detail>
        <NavigationPage>
            <x:Arguments>
                <local:MainPage />
            </x:Arguments>
        </NavigationPage>
    </FlyoutPage.Detail>
</FlyoutPage>
